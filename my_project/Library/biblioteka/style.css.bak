html {
height: 100%;
margin: 0;
padding: 0;  
}

body{
padding:0px; 
margin:0px; 
display: flex;
flex-direction: column;
height: 100%;
font:13px/14px Arial, Helvetica, sans-serif;
list-style-type:none;
}


div, p, ul, h2, h3, form, label, select, img{
padding:0px; 
margin:0px;
}
ul{
list-style-type:none;
}

.example{
	position: relative;
	left:19rem;
	top:-40px;
  font-size: 17px;
}

form.example button:hover {
  background: #0b7dda;
}

/* Clear floats */
form.example::after {
  content: "";
  clear: both;
  display: table;
}

